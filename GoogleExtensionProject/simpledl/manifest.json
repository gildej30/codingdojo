{
    "name": "Simple Youtube Downloader",
    "description": "Download Youtube videos stress free.",
    "version": "0.1",
    "author": "Jason Gildenzopf",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/watch*"],
            "js": ["src/youtubedl.js"]
        }
    ]
}